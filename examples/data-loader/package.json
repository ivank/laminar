{
  "name": "@examples/data-loader",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "description": "This is a simple but functional example of building a data loading api.",
  "author": "Ivan Kerin <ikerin@gmail.com>",
  "repository": "git@github.com:ivank/laminar.git",
  "homepage": "https://github.com/ivank/laminar/tree/main/examples/todo#readme",
  "license": "Apache-2.0",
  "devDependencies": {
    "@laminar/cli": "*",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "axios": "^1.6.2",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-retry-promise": "^0.7.1",
    "typescript": "^5.2.2"
  },
  "jest": {
    "preset": "../../jest.json"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "create-session": "node dist/create-session.js",
    "build:schema": "laminar api --file schema.yaml --output src/__generated__/schema.ts",
    "build:avro": "laminar avro src/services/consumers/avro/*.json --output-dir src/__generated__ --logical-type-import-default decimal=Decimal:decimal.js  --logical-type timestamp-millis=Date",
    "pretest": "laminar axios --file schema.yaml --output __generated__/schema.axios.ts",
    "build": "yarn build:avro && yarn build:schema && yarn tsc",
    "test": "KAFKAJS_NO_PARTITIONER_WARNING=1 TZ=UTC jest test --runInBand",
    "lint": "F='{src,test}/**/*.ts' && prettier --check $F && eslint $F"
  },
  "dependencies": {
    "@kafkajs/confluent-schema-registry": "^3.3.0",
    "@laminar/avro": "^0.12.2",
    "@laminar/jwt": "*",
    "@laminar/kafkajs": "*",
    "@laminar/laminar": "*",
    "@laminar/pg": "*",
    "@laminar/pgboss": "*",
    "@laminar/winston": "*",
    "avsc": "^5.7.7",
    "csv-parse": "^5.5.2",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.3.1",
    "kafkajs": "^2.2.4",
    "pg": "^8.11.3",
    "pg-boss": "^9.0.3",
    "runtypes": "^6.7.0",
    "sql-template-tag": "^4.0.0",
    "winston": "^3.11.0"
  }
}
