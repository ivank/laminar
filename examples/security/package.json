{
  "name": "@examples/security",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "description": "This is a simple example of a laminar app split into files",
  "author": "Ivan Kerin <ikerin@gmail.com>",
  "repository": "git@github.com:ovotech/laminar.git",
  "homepage": "https://github.com/ovotech/laminar/tree/main/examples/security#readme",
  "license": "Apache-2.0",
  "devDependencies": {
    "@ovotech/laminar-cli": "*",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "axios": "^1.6.2",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "jest": {
    "preset": "../../jest.json"
  },
  "scripts": {
    "build:schema": "laminar api --file src/api.yaml --output src/__generated__/api.ts",
    "build:ts": "tsc --declaration",
    "build": "yarn build:schema && yarn build:ts",
    "start": "ts-node src/index.ts",
    "test": "jest test --runInBand",
    "lint": "F='{src,test}/**/*.ts' && prettier --check $F && eslint $F"
  },
  "dependencies": {
    "@ovotech/laminar": "*",
    "@ovotech/laminar-jwt": "*"
  }
}
